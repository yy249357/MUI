body,div,ul,li,span,a,img,p,h1,h2,h3,h4,h5,h6,em,i{padding:0; margin:0;}
h1,h2,h3,h4,h5{font-weight: normal;}
a{text-decoration:none;}
img{vertical-align: middle;}
a,img{border:none;}
ul,li{list-style:none;}
input{padding:0;}
a,button,input{-webkit-tap-highlight-color:rgba(255,0,0,0);-webkit-appearance: none;}
a,a:visited,a:link,a:hover,a:active{color:#333;}
button{outline:none;}
body{font: 14px/1.5 Helvetica, "Microsoft Yahei", Arial, sans-serif;color:#666; -webkit-font-smoothing: subpixel-antialiased;}


