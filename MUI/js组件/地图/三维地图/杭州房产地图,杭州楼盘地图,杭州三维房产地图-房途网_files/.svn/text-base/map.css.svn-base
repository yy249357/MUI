html,body{height:100%;}
body{font-size:12px; background:none repeat scroll 0 0 #FFFFFF; color:#333333}
.container{ margin:0px; padding:0px; position:absolute; width:100%; height:100%; top:0px;left:0px; right:0px; bottom:0px; overflow:hidden; display:none;top:100%; background:#fff;  }
.lmsg{ position:absolute; display:none; top:40%; width:100%;  left:0px; z-index:9999; text-align:center;}
.ltip{ margin:5px 0px; color:#6c6c6c;}
.lbg_loading{ background:url("../images/long_loading.gif") no-repeat; width:100px; height:9px; margin:0px auto;}

.menu{ height:100%; position:relative; z-index:1; overflow:hidden;}
.sys_menu{z-index:2; position:absolute;top:0px; right:3px; height:31px; overflow:hidden; }
.sys_menu ul{ font-size:14px;}
.sys_menu ul li{ height:31px; float:left; line-height:31px; padding:0px 10px; background: url("../images/homenav_08.gif") repeat-x scroll 0 0; }
.sys_menu ul li a{color: #FFFFFF;}
.sys_menu ul li.split{width:3px; padding:0px;background:url("../images/homenav_09.gif") no-repeat scroll right center;}
.sys_menu ul li.first{width:23px;padding:0px;background:url("../images/homenav_14.gif") no-repeat scroll left center;}
.sys_menu ul li.last{width:23px;padding:0px;background:url("../images/homenav_15.gif") no-repeat scroll left center;}

.map_menu {position:absolute;bottom:5px;left:115px;z-index:1; height:30px;overflow:hidden; }
.map_menu ul{ font-size:15px;}
.map_menu ul li{ float:left; height:30px; line-height:30px;cursor:pointer; }
.map_menu ul li a{display:inline-block; width:74px; text-align:center; height:100%;padding:0px 10px; font-size:16px; font-family:微软雅黑;}
.map_menu ul li a.selected{ color:#ffffff;background:url("../images/map_icon_06.png") no-repeat scroll left center;}

.map_nav{ border:1px solid #E9E9E9;background:url("../images/bg_map_nav.png") repeat-x scroll 0 0 ; height:32px; color:#6c6c6c; }
.left_nav,.right_nav{ height:32px; line-height:32px;}
.map_nav li{ float:left;}
.nav_split{ border-left:1px solid #dfdfdf;border-right: 1px solid #fff;overflow: hidden; width:0px; display:inline-block; height:15px; margin-top:8px; }
.map_nav .item{ margin:0px 10px; cursor:pointer;}
.map_nav .itemico{ margin:0px 10px; cursor:pointer; height:32px; line-height:32px; display:inline-block;}
.map_nav .itemico{ _margin:0px; _display:inline-block;_padding:10px 10px 5px;_height:100%; }

.near-item{ display:inline-block; position:relative; top:3px; }
.dropdownlist.selected{ color:red;}
.nav_panel{ background:#fff; border:1px solid #e9e9e9; width:100px; padding:5px;}
.nav_panel li{ margin:3px 0px;}
.nav_panel label{height:18px; line-height:18px;display:inline-block; padding-left:20px;}
.icon_xq{background:url("../images/icon002.gif") no-repeat; background-position:0px 0px;}
.icon_gj{background:url("../images/icon002.gif") no-repeat; background-position:0px -20px;}
.icon_cy{background:url("../images/icon002.gif") no-repeat; background-position:0px -40px;}
.icon_yh{background:url("../images/icon002.gif") no-repeat; background-position:0px -60px;}
.icon_cs{background:url("../images/icon002.gif") no-repeat; background-position:0px -80px;}
.icon_sc{background:url("../images/icon002.gif") no-repeat; background-position:0px -100px;}
.icon_xx{background:url("../images/icon002.gif") no-repeat; background-position:0px -120px;}
.icon_yy{background:url("../images/icon002.gif") no-repeat; background-position:0px -140px;}
.icon_jj{background:url("../images/icon002.gif") no-repeat; background-position:0px -160px;}
.icon_zs{background:url("../images/icon002.gif") no-repeat; background-position:0px -180px;}
.icon_jyz{background:url("../images/icon002.gif") no-repeat; background-position:0px -200px;}

/*结果消息显示*/
.resultMsg{ color:#5C5C5C; background-color:#F6FCFF; overflow:hidden;_height:1%;zoom:1; height: 32px;line-height: 32px;  }
.resultMsg .content{font-size:12px; margin-left:5px;}
.totalNum{ margin:0px 3px; color:#f50;}
.back{ cursor:pointer; color:#008DD7;margin-right:10px;}
.result{ margin: 8px;}
.result li.plan{  border:1px solid #9CB5FF; margin-bottom:10px;  }
li.plan .tranTitle { overflow:hidden;_height:1%;zoom:1;  height:30px; line-height:30px;cursor:pointer;}
li.plan .tranTitle .floatl{color:#3D6DCC; font-size:14px; margin-left:5px;}
li.plan .tranTitle .floatr{color:#808080;margin-right:5px;}
li.plan .steps{ border-top:1px solid #9CB5FF; display:none; color:#333333;}
li.plan .steps td{padding:5px 3px; margin:3px 0px;cursor:pointer;}
li.plan .steps .td_ignored td{cursor:default;}
li.plan .steps b{color:#3D6DCC; font-weight:normal;}
.td_ignored{color:#333333;font-weight:bold;}
.ico_start{ display:block; height:28px; line-height:28px; background:url("../images/bus_trans_bg_109fe77.jpg") no-repeat; background-position:2px 2px;}
.ico_end{display:block;height:28px; line-height:26px; background:url("../images/bus_trans_bg_109fe77.jpg") no-repeat; background-position:2px -26px}
.ico_walk{display:block;width:28px; height:18px; background:url("../images/bgs_ico.gif") no-repeat; background-position:-21px -25px;}
.ico_bus{display:block;width:28px; height:15px; background:url("../images/bgs_ico.gif") no-repeat;  background-position:6px -25px;}
.ico_car{display:block;width:17px; height:15px; background:url("../images/bgs_ico.gif") no-repeat; background-position:-84px -5px; margin-right:11px; margin-left:2px; }
.bus{color:#3D6DCC; font-weight:bold; padding:0px 2px;}
.busStation{color:#3D6DCC;padding:0px 2px;}
.stopNum,.meter{ text-decoration:underline;padding:0px 2px; color:#3c3c3c;}
li.plan.selected .tranTitle{background-color:#EBF1FB; cursor:pointer;}
li.plan.selected .tranTitle .floatl{ font-weight:bold; }
li.plan.selected .steps{ display:block;}
li.plan .steps tr.hover{ background:#EBF1FB}
li.plan .steps tr.selected{ background:#EBF1FB;color:#333333; }

.con{ margin:10px; color:#6C6C6C; font-size:13px; /**overflow:hidden;_height:1%;zoom:1;*/}
.con a{ color:#1166BB;}
.con a:hover{color:#f50;}
.con a.selected{ background:red; }

/*非主要条件*/
.con .con_title{ letter-spacing:1px; font-size:13px;display:table-cell; white-space:nowrap; *float:left; *padding-top:2px; }
.con .con_content{ display:table-cell; *float:left;}
.con .con_content span{ white-space:nowrap; padding:2px; display: inline-block; margin-bottom:4px; }
.con_split .con_content span a{ color:#666;}
.con_split .con_content span.selected a{ color:#fff;}
.con .con_split{ margin-top:3px; border-bottom:1px dashed #e9e9e9; margin-bottom:5px; *overflow:hidden;_overflow:inherit;_height:1%;zoom:1; }
.con .con_split.b{ border-bottom:0px none;}
.search_panel{position:relative; overflow:hidden;height:30px;zoom:1;}
.hotarea{ font-size:11px; color:#a3a3a3;}
.con .results{ border:1px solid #d2d2d2; overflow:auto; border-top:0px none; background-color:#F9FEFF;}
.con .results li{ margin-bottom:5px; padding:3px; cursor:pointer;}

.con .line{ border-bottom:2px solid #1F8DD0;}
.con .line .title{ color:#666; font-weight:bold; font-size:14px; padding:3px;}
.tips{ padding:5px; font-size:12px; margin-top:15px; border:1px solid #DAC19A; background-color:#FFFCEC;}
.tips h3{ font-size:14px; font-weight:bold; background:url("../images/main_icon.jpg")  no-repeat; background-position: -3px -1081px; padding: 5px 0 8px 17px;}
.tips ol{ padding: 5px 5px 0 23px}
.tips li{ margin-bottom:10px;}

.con fieldset{border:#D2D2D2 solid 1px; background:#F9FEFF;}
.con legend {margin:0px 2px;}
.con fieldset .con_content,.con fieldset table{ *margin-top:3px;*margin-left:9px;}
.con.mt{ margin-top:15px;}
.con.property{ overflow:hidden;zoom:1; padding-top:5px; }
.con.property ul{ overflow:hidden;_height:1%;zoom:1; padding-top:5px; }
.con.property li{white-space:nowrap; width:49%; font-size:13px; float:left; position:relative; border-left:1px dashed #fff;border-right:1px dashed #d2d2d2; border-bottom:1px dashed #d2d2d2;}
.con.property li.last{border-left:0px none; border-right:1px dashed #fff;}
.con.property li.bottom{ border-bottom:0px none;}
.con.property li div{ padding:8px 0px 8px 5px; line-height:20px; height:20px;position:relative; }
.con .arrow{ background:url('../images/map_icon_93.jpg') no-repeat; width:4px; height:7px; display:block; position:absolute; top:45%; right:8px; }
.con .msginput{ position:absolute; border:none; border:1px solid #d2d2d2; color:#6c6c6c; padding:3px;line-height:22px;height:22px; width:223px;}
.con .msginput.on{ color:#232323;}
.con .btnsearch{ cursor:pointer; position:absolute;border:none; background:url("../images/map_icon_47.jpg") no-repeat; width:60px; height:30px; right:0px;}
.con.property li.selected{color:#008DD7;position:relative; border:1px solid #cee5ff; background-color:#F9FEFF;  border-top:1px solid #cee5ff; border-bottom:0px none; z-index:999;}
.con.property li.selected.l{}
.con.property li.selected.r{ margin-left:-2px; }
.con.property li.selected.l.t{ margin-top:-1px;}
.con.property li.selected.r.t{ margin-top:-1px;}
.con.property li.selected.bottom{ margin-top:-1px;}
.con.property li.selected .arrow{ background:url('../images/map_icon_94.jpg') no-repeat;width:7px;}
.con.property .selection{color:Red;  font-size:11px; margin-left:5px;}

.property_panel{  position:absolute; background:#F9FEFF; width:100%; z-index:998; border:1px solid #cee5ff; display:none; width:274px; padding:5px;}
.property_panel .items{display:none;white-space:normal; }
.property_panel span{ white-space:nowrap; padding:3px; display:inline-block; margin:3px 0px 3px 3px;}
.property_panel span a{ color:#1166BB;}

/*列表*/
.more{ width:100%; display:none; text-align:center; margin:10px 0px;}
.more a{ color:#258FCB; font-size:13px;}
.more a:hover{ color:#f50;}
.pagenum{ height:25px; line-height:25px; font-size:12pt; font-weight:bold; color:#DB6918; text-align:center;}
.houseItem{ color:#6c6c6c;  margin:5pt 0pt; overflow:hidden;}
.housePic{ margin-left:2px; overflow:hidden; padding:3px; border:1pt solid #c7c7c7; background:#ffffff; width:85pt; height:65pt;}
.housePic img{ width:100%; height:100%;}
.houseProperty{ margin-right:2pt;}
.houseProperty .p{margin-top:6pt;}
.houseProperty .ptag{margin-top:8pt; }
.commend{ font-size:13px; font-weight:bold;}
.price{ float:right;right:10px;font-size:13px;}
.price .orangeNum{font-size:15px; color:#f50; font-weight:bold;}
.tag{color:#ffffff; padding:1px 2px; margin-right:3px;}
.rw{ background-color:#DA3306;}
.qw{background-color:#17B2B6;}
.gw{ background-color:#619D44;}
.bw{background-color:#3D8DD2;}
.yw{background-color:#F38B38;}
.pw{background-color:#FF69B4;}
.list{ background:#fff;}

span.selected{ background-color:#258FCB; cursor:default;}
span.selected a{ color:#fff;}
.con span a:hover{color:#f50;}
.results ul li.selected{ background:#EBF1FB;}
.results ul li.selected a{ color:#ff0000; font-weight:bold;}

/*左边内容显示与隐藏*/
.content_items{ display:none;}

/*去除a标签虚线框*/
a {outline: none;}
a:active {*star:expression(this.onFocus=this.blur());}
:focus { outline:0; }

/*tab效果*/
.tabline{border-bottom:2px solid #1F8DD0; overflow:hidden; height:20px; }
.tabs{height: 100%px;list-style: none outside none;position: relative;width:100%;  }
.tabs li{float: left;width: 35px; height:1px; margin-right:12px;}
.tabs li a{width: 35px;border-bottom: 20px solid #4c4c4c;border-right: 20px solid transparent;_border-right: 20px solid #ffffff;border-top: 0px none;color: #FFFFFF;position: absolute;text-decoration: none;opacity: .4;filter: alpha(opacity=40);}
.tabs li a.selected{  border-bottom: 20px solid #1F8DD0;opacity: 1;filter: alpha(opacity=100);z-index:3;_border-right-color:tomato;_filter:chroma(color=tomato); background:none;}
.tabs li a span{ cursor:pointer; display: block;left: 6px;position: absolute;top: 3px;*top:5px;}
.tabs li a.selected span{cursor:default;}
.tabs a:hover{border-bottom-color: #1F8DD0; color:#fff;}

/*pager效果*/
.page{ border:0px none;}
.page li{ height:auto;line-height:normal;}
.page .pgNext,.page .pgPrev{   padding:2px 3px;}
.page .pgPrev,.page .pgNext{border:0px none;}
.page a,.page .page-number{ width:auto;}
.page .page-number{ width:25px; padding:2px ;}
.page .pgCurrent{ height:auto; background:none; background-color:#008DD7; }
.page .pgCurrent a{color:#fff;}

/*地图*/
.map_op{position:absolute; right:5px; top:5px;z-index: 999999;}
.map_op span{ padding:0px 5px; height:20px; line-height:20px; display:inline-block; border:1px solid #d2d2d2; cursor:pointer; background-color:#000; opacity:.7; filter:alpha(opacity=70);color:#fff;}
.map_op span.selected{ background-color:#008DD7;}

.mapEye{background-color:#fff; border:1px solid #d2d2d2; overflow:hidden;  position:absolute; padding:3px; width:124px; height:100px; right:0px; bottom:0px; z-index:999999;}
.mapEye.in{ width:350px; height:220px;}
.mapEye .baiduMap{border:1px solid #d2d2d2; }

/* all 'panes' */ 
.ui-layout-pane { background: #FFF; border: 1px solid #E9E9E9; overflow: auto;} 
 /* all 'resizer-bars' */ 
.ui-layout-resizer {background: #F9F9F9; } 
/* all 'toggler-buttons' #aaa*/ 
.ui-layout-toggler {background: #208CD1; } 
.ui-layout-pane-north{ border:0px none;}
.ui-layout-pane-west,.ui-layout-pane-center{ border:0px none; overflow:hidden; background-color:#fff; position:relative;}

.anchorBL{ display:none;}
