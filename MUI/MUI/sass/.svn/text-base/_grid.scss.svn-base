.row {
    margin: 0 auto;
    width: 100%;
    *zoom: 1;
}
.row:before,.row:after {
    content: " ";
    display: table;
}
.row:after {
    clear: both;
}
[class*="grid"] {
    width: 100%;
    float: left;
    position: relative;
}
[class*="am-u-"]:last-child {
    float: right;
}
.grid1 {
    width: 8.33333333%;
}
.grid2 {
    width: 16.66666667%;
}
.grid3 {
    width: 25%;
}
.grid4 {
    width: 33.33333333%;
}
.grid5 {
    width: 41.66666667%;
}
.grid6 {
    width: 50%;
}
.grid7 {
    width: 58.33333333%;
}
.grid8 {
    width: 66.66666667%;
}
.grid9 {
    width: 75%;
}
.grid10 {
    width: 83.33333333%;
}
.grid11 {
    width: 91.66666667%;
}
.grid12 {
    width: 100%;
}

[class*="avg"] {
  display: block;
  padding: 0;
  margin: 0;
  list-style: none;
}
[class*="avg"]:before,
[class*="avg"]:after {
  content: " ";
  display: table;
}
[class*="avg"]:after {
  clear: both;
}
[class*="avg"] > li {
  display: block;
  height: auto;
  float: left;
}
.avg1 > li {
    width: 100%;
}
.avg1 > li:nth-of-type(n) {
    clear: none;
}
.avg1 > li:nth-of-type(1n + 1) {
    clear: both;
}
